{
  "title": {
    "text": "Epidemic trajectory"
  },
  "shapes": params[].phases#$pos[$pos!=0].{
    "type": "line",
    "x0": start,
    "y0": 0,
    "x1": start,
    "y1": $max($$.data[visible=true or $not($exists(visible))].y),
    "line": {
      "color": "black",
      "width": 1,
      "dash": "dot"
    }
  },
  "yaxis": $merge([{
    "autorange": false,
    "hoverformat": ",f"
  }, params.log ? (
  $nticks := $length($string($floor($max(data[visible=true or $not($exists(visible))].y)))) + 1;
  {
    "type": "log",
    "tickmode": "array",
    "tickvals": [0..$nticks - 1].($power(10, $)),
    "range": [
      0,
      $nticks
    ]
  }) : {
    "type": "linear",
    "range": [
      0,
      1.1 * $max(data[visible=true or $not($exists(visible))].y)
    ]
  }]),
  "xaxis": {
    "autorange": false,
    "range": [
      "2020-01-01",
      "2023-01-01"
    ]
  }
}
